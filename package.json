{
    "name": "remix-start",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/riipandi/remix-start.git"
    },
    "bugs": {
        "url": "https://github.com/riipandi/remix-start/issues"
    },
    "homepage": "https://github.com/riipandi/remix-start#readme",
    "config": {
        "image": "ghcr.io/riipandi/remix-start"
    },
    "scripts": {
        "build": "react-router build",
        "dev": "NODE_ENV=development node -r dotenv/config server.js",
        "start": "node -r dotenv/config server.js",
        "dev:storybook": "VITE_CJS_IGNORE_WARNING=true storybook dev -p 6006 --no-open",
        "build:storybook": "VITE_CJS_IGNORE_WARNING=true storybook build -o storybook-static",
        "start:storybook": "pnpm dlx serve storybook-static -l 6006 -n -C",
        "docker:build": "docker build -f Dockerfile . -t $npm_package_config_image:latest -t $npm_package_config_image:$npm_package_version",
        "docker:push": "docker push $npm_package_config_image:latest && docker push $npm_package_config_image:$npm_package_version",
        "docker:run": "pnpm --silent run dockerx --name $npm_package_name $npm_package_config_image:$npm_package_version",
        "docker:shell": "pnpm --silent run dockerx --entrypoint /bin/sh $npm_package_config_image:$npm_package_version",
        "docker:images": "docker image list --filter reference=$npm_package_config_image:*",
        "dockerx": "docker run --network=host --rm -it --env-file .env -v $(pwd)/_data:/srv/_data",
        "check": "biome check . --write",
        "format": "biome format . --write",
        "lint": "biome lint . --write",
        "postinstall": "react-router typegen",
        "update-deps": "npm-check-updates --configFileName .ncurc.json",
        "cleanup": "pnpm --stream --parallel --color \"/cleanup:(?!data)/\"",
        "cleanup:dev": "pnpm dlx rimraf .{react-router,temp} dist storybook-static",
        "cleanup:deps": "pnpm dlx rimraf pnpm-lock.yaml node_modules",
        "cleanup:test": "pnpm dlx rimraf tests-results playwright-report",
        "cleanup:data": "pnpm dlx rimraf _data",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@radix-ui/react-slot": "^1.1.1",
        "@react-router/express": "^7.1.3",
        "@react-router/node": "^7.1.3",
        "compression": "^1.7.5",
        "consola": "^3.4.0",
        "express": "^5.0.1",
        "isbot": "^5.1.22",
        "lucide-react": "^0.474.0",
        "morgan": "^1.10.0",
        "pathe": "^2.0.2",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-router": "^7.1.3",
        "std-env": "^3.8.0"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.9.4",
        "@react-router/dev": "^7.1.3",
        "@storybook/addon-a11y": "^8.5.2",
        "@storybook/addon-essentials": "^8.5.2",
        "@storybook/addon-links": "^8.5.2",
        "@storybook/addon-storysource": "^8.5.2",
        "@storybook/blocks": "^8.5.2",
        "@storybook/core-events": "^8.5.2",
        "@storybook/manager-api": "^8.5.2",
        "@storybook/react": "^8.5.2",
        "@storybook/react-vite": "^8.5.2",
        "@storybook/test": "^8.5.2",
        "@storybook/theming": "^8.5.2",
        "@tailwindcss/typography": "^0.5.16",
        "@tailwindcss/vite": "^4.0.0",
        "@types/compression": "^1.7.5",
        "@types/express": "^5.0.0",
        "@types/express-serve-static-core": "^5.0.6",
        "@types/morgan": "^1.9.9",
        "@types/node": "^22.12.0",
        "@types/react": "^19.0.8",
        "@types/react-dom": "^19.0.3",
        "clsx": "^2.1.1",
        "dotenv": "^16.4.7",
        "npm-check-updates": "^17.1.14",
        "rollup-plugin-visualizer": "^5.14.0",
        "storybook": "^8.5.2",
        "tailwind-variants": "^0.3.1",
        "tailwindcss": "^4.0.0",
        "tailwindcss-motion": "^1.0.1",
        "typescript": "^5.7.3",
        "vite": "^6.0.11",
        "vite-tsconfig-paths": "^5.1.4"
    },
    "packageManager": "pnpm@9.15.4"
}
