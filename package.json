{
    "name": "remix-start",
    "version": "0.0.0",
    "private": true,
    "sideEffects": false,
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/riipandi/remix-start.git"
    },
    "bugs": {
        "url": "https://github.com/riipandi/remix-start/issues"
    },
    "homepage": "https://github.com/riipandi/remix-start#readme",
    "scripts": {
        "build": "remix vite:build",
        "dev": "remix vite:dev",
        "start": "remix-serve ./build/server/index.js",
        "preview": "dotenv -e .env -- remix-serve ./build/server/index.js",
        "dev:storybook": "storybook dev -p 6006 --no-open",
        "build:storybook": "storybook build -o storybook-static",
        "lint": "biome lint . --write",
        "check": "biome check . --write",
        "format": "biome format . --write",
        "docker:build": "docker build -f Dockerfile . -t $npm_package_name:latest -t $npm_package_name:$npm_package_version",
        "docker:run": "docker run --rm -it -p 3000:3000 --env-file .env --name $npm_package_name $npm_package_name:latest",
        "docker:shell": "docker run --rm -it -p 3000:3000 --env-file .env --entrypoint /bin/sh $npm_package_name:$npm_package_version",
        "update-deps": "npm-check-updates --configFileName .ncurc.json",
        "cleanup": "docker compose down --remove-orphans --volumes",
        "preinstall": "pnpm dlx only-allow pnpm",
        "pre-dev": "docker compose up -d",
        "pre-commit": "sh .husky/pre-commit",
        "prepare": "husky || true",
        "test": "vitest --run",
        "test:ui": "vitest --ui",
        "coverage": "vitest run --coverage",
        "e2e-test:ui": "playwright test --ui",
        "e2e-test:desktop": "playwright test --headed --project Chromium",
        "e2e-test:mobile": "playwright test --headed --project 'Mobile Chrome'",
        "e2e-test:install": "playwright install-deps",
        "e2e-test:report": "playwright show-report",
        "typecheck": "tsc -b"
    },
    "dependencies": {
        "@remix-run/css-bundle": "^2.11.2",
        "@remix-run/node": "^2.11.2",
        "@remix-run/react": "^2.11.2",
        "@remix-run/serve": "^2.11.2",
        "clsx": "^2.1.1",
        "isbot": "^5.1.17",
        "lucide-react": "^0.438.0",
        "react-dom": "^18.3.1",
        "react": "^18.3.1",
        "remix-flat-routes": "^0.6.5",
        "tailwind-merge": "^2.5.2",
        "tailwind-variants": "^0.2.1"
    },
    "devDependencies": {
        "@biomejs/biome": "1.8.3",
        "@chromatic-com/storybook": "^1.8.0",
        "@playwright/test": "^1.46.1",
        "@remix-run/dev": "^2.11.2",
        "@remix-run/testing": "^2.11.2",
        "@storybook/addon-essentials": "^8.2.9",
        "@storybook/addon-interactions": "^8.2.9",
        "@storybook/addon-links": "^8.2.9",
        "@storybook/blocks": "^8.2.9",
        "@storybook/react-vite": "^8.2.9",
        "@storybook/react": "^8.2.9",
        "@storybook/test": "^8.2.9",
        "@storybook/theming": "^8.2.9",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/forms": "^0.5.8",
        "@tailwindcss/typography": "^0.5.15",
        "@testing-library/jest-dom": "^6.5.0",
        "@testing-library/react": "^16.0.1",
        "@types/jest": "^29.5.12",
        "@types/node": "^22.5.3",
        "@types/react-dom": "^18.3.0",
        "@types/react": "^18.3.5",
        "@vitest/coverage-v8": "^2.0.5",
        "@vitest/ui": "^2.0.5",
        "autoprefixer": "^10.4.20",
        "dotenv-cli": "^7.4.2",
        "dotenv": "^16.4.5",
        "happy-dom": "^15.7.3",
        "husky": "^9.1.5",
        "npm-check-updates": "^17.1.1",
        "playwright": "^1.46.1",
        "postcss": "^8.4.44",
        "rollup-plugin-visualizer": "^5.12.0",
        "storybook": "8.2.9",
        "tailwind-debug-breakpoints": "^1.0.3",
        "tailwindcss-animate": "^1.0.7",
        "tailwindcss": "^3.4.10",
        "typescript": "^5.1.6",
        "vite-tsconfig-paths": "^5.0.1",
        "vite": "^5.4.3",
        "vitest": "^2.0.5"
    },
    "engines": {
        "node": ">=20.11.1"
    },
    "packageManager": "pnpm@9.9.0"
}
