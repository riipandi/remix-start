{
    "name": "remix-start",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/riipandi/remix-start.git"
    },
    "bugs": {
        "url": "https://github.com/riipandi/remix-start/issues"
    },
    "homepage": "https://github.com/riipandi/remix-start#readme",
    "config": {
        "image": "ghcr.io/riipandi/remix-start"
    },
    "scripts": {
        "build:app": "react-router build",
        "build": "pnpm run --stream --color \"/build:(app|storybook)/\"",
        "start": "node --no-warnings -r dotenv/config server.js",
        "dev": "NODE_ENV=development node -r dotenv/config server.js",
        "dev:storybook": "VITE_CJS_IGNORE_WARNING=true storybook dev -p 6006 --no-open",
        "build:storybook": "VITE_CJS_IGNORE_WARNING=true storybook build -o storybook-static",
        "start:storybook": "pnpm dlx serve storybook-static -l 6006 -n -C",
        "docker:build": "docker build -f Dockerfile . -t $npm_package_config_image:latest -t $npm_package_config_image:$npm_package_version",
        "docker:push": "docker push $npm_package_config_image:latest && docker push $npm_package_config_image:$npm_package_version",
        "docker:run": "pnpm --silent run dockerx --name $npm_package_name $npm_package_config_image:$npm_package_version",
        "docker:shell": "pnpm --silent run dockerx --entrypoint /bin/sh $npm_package_config_image:$npm_package_version",
        "docker:images": "docker image list --filter reference=$npm_package_config_image:*",
        "dockerx": "docker run --network=host --rm -it --env-file .env -v $(pwd)/_data:/srv/_data",
        "compose:cleanup": "docker compose -f compose.yaml down --remove-orphans --volumes",
        "compose:up": "docker compose -f compose.yaml up --detach --remove-orphans",
        "compose:down": "docker compose -f compose.yaml down --remove-orphans",
        "generate:key": "openssl rand -base64 500 | tr -dc 'a-zA-Z0-9' | fold -w 40 | head -n 1",
        "generate:changelog": "git-cliff -o CHANGELOG.md -c cliff-full.toml",
        "pre-commit": "lefthook run pre-commit --force",
        "check": "biome check . --write",
        "format": "biome format . --write",
        "lint": "biome lint . --write",
        "postinstall": "react-router typegen",
        "prepare": "lefthook install || true",
        "update-deps": "npm-check-updates --configFileName .ncurc.json",
        "cleanup": "pnpm --stream --parallel --color \"/cleanup:(?!data)/\"",
        "cleanup:dev": "pnpm dlx rimraf .{react-router,temp} dist storybook-static",
        "cleanup:deps": "pnpm dlx rimraf pnpm-lock.yaml node_modules",
        "cleanup:test": "pnpm dlx rimraf tests-results playwright-report",
        "cleanup:data": "pnpm dlx rimraf _data",
        "e2e-test:chrome": "playwright test --headed --project Chromium",
        "e2e-test:firefox": "playwright test --headed --project Firefox",
        "e2e-test:install": "playwright install --with-deps",
        "e2e-test:mobile": "playwright test --headed --project 'Mobile Chrome'",
        "e2e-test:report": "playwright show-report playwright-report",
        "e2e-test:safari": "playwright test --headed --project Safari",
        "e2e-test:ui": "playwright test --ui",
        "test:coverage": "vitest run --coverage",
        "test:report": "pnpm dlx http-server tests-results --port 0 -a localhost",
        "test:ui": "vitest --ui",
        "test": "vitest --run",
        "typecheck": "tsc -b --noEmit"
    },
    "dependencies": {
        "@radix-ui/react-slot": "^1.1.1",
        "@react-router/express": "^7.1.3",
        "@react-router/node": "^7.1.3",
        "compression": "^1.7.5",
        "consola": "^3.4.0",
        "express": "^5.0.1",
        "isbot": "^5.1.22",
        "lucide-react": "^0.474.0",
        "morgan": "^1.10.0",
        "pathe": "^2.0.2",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-router": "^7.1.3",
        "std-env": "^3.8.0"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.9.4",
        "@react-router/dev": "^7.1.3",
        "@storybook/addon-a11y": "^8.5.2",
        "@storybook/addon-essentials": "^8.5.2",
        "@storybook/addon-links": "^8.5.2",
        "@storybook/addon-storysource": "^8.5.2",
        "@storybook/blocks": "^8.5.2",
        "@storybook/core-events": "^8.5.2",
        "@storybook/manager-api": "^8.5.2",
        "@storybook/react-vite": "^8.5.2",
        "@storybook/react": "^8.5.2",
        "@storybook/test": "^8.5.2",
        "@storybook/theming": "^8.5.2",
        "@tailwindcss/typography": "^0.5.16",
        "git-cliff": "^2.8.0",
        "@tailwindcss/vite": "^4.0.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.2.0",
        "@types/compression": "^1.7.5",
        "@types/express-serve-static-core": "^5.0.6",
        "@types/express": "^5.0.0",
        "@types/morgan": "^1.9.9",
        "@types/node": "^22.12.0",
        "lefthook": "^1.10.10",
        "@types/react-dom": "^19.0.3",
        "@types/react": "^19.0.8",
        "@playwright/test": "^1.50.0",
        "playwright": "^1.50.0",
        "@vitest/coverage-istanbul": "^3.0.4",
        "@vitest/ui": "^3.0.4",
        "clsx": "^2.1.1",
        "dotenv": "^16.4.7",
        "happy-dom": "^16.7.3",
        "npm-check-updates": "^17.1.14",
        "rollup-plugin-visualizer": "^5.14.0",
        "storybook": "^8.5.2",
        "tailwind-variants": "^0.3.1",
        "tailwindcss-motion": "^1.0.1",
        "tailwindcss": "^4.0.0",
        "typescript": "^5.7.3",
        "vite-tsconfig-paths": "^5.1.4",
        "vite": "^6.0.11",
        "vitest": "^3.0.4"
    },
    "packageManager": "pnpm@9.15.4"
}
