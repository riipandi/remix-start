{
    "name": "remix-start",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/riipandi/remix-start.git"
    },
    "bugs": {
        "url": "https://github.com/riipandi/remix-start/issues"
    },
    "homepage": "https://github.com/riipandi/remix-start#readme",
    "config": {
        "image": "ghcr.io/riipandi/remix-start"
    },
    "scripts": {
        "build:app": "react-router build",
        "build": "pnpm run --stream --color \"/build:(app|storybook)/\"",
        "start": "node --no-warnings -r dotenv/config server.js",
        "dev": "NODE_ENV=development node -r dotenv/config server.js",
        "dev:storybook": "storybook dev -p 6006 --no-open",
        "build:storybook": "storybook build -o storybook-static",
        "start:storybook": "pnpm dlx serve storybook-static -l 6006 -n -C",
        "dockerx": "docker run --platform=linux/amd64 --network=host --rm -it --env-file .env -v $(pwd)/storage:/srv/storage",
        "docker:build": "docker build -f Dockerfile . --build-arg DISTROLESS_TAG=debug-nonroot -t $npm_package_config_image:$npm_package_version-dev",
        "docker:push": "docker push $npm_package_config_image:latest-dev && docker push $npm_package_config_image:$npm_package_version-dev",
        "docker:run": "pnpm --silent run dockerx --name $npm_package_name $npm_package_config_image:$npm_package_version-dev",
        "docker:shell": "pnpm --silent run dockerx --entrypoint /busybox/sh $npm_package_config_image:$npm_package_version-dev",
        "docker:prune": "docker image rm --force $(docker image list --filter reference=$npm_package_config_image:* -q)",
        "docker:images": "docker image list --filter reference=$npm_package_config_image:*",
        "compose:cleanup": "docker compose -f compose.yaml down --remove-orphans --volumes",
        "compose:up": "docker compose -f compose.yaml up --detach --remove-orphans",
        "compose:down": "docker compose -f compose.yaml down --remove-orphans",
        "generate:key": "openssl rand -base64 500 | tr -dc 'a-zA-Z0-9' | fold -w 40 | head -n 1",
        "generate:changelog": "git-cliff -o CHANGELOG.md -c cliff-full.toml",
        "pre-commit": "lefthook run pre-commit --force",
        "check": "biome check . --write",
        "format": "biome format . --write",
        "lint": "biome lint . --write",
        "prepare": "lefthook install || true",
        "postinstall": "react-router typegen",
        "update-deps": "npm-check-updates --configFileName .ncurc.json",
        "cleanup": "pnpm --stream --parallel --color \"/cleanup:(?!data)/\"",
        "cleanup:dev": "pnpm dlx del-cli .{react-router,wrangler} dist storybook-static",
        "cleanup:deps": "pnpm dlx del-cli pnpm-lock.yaml node_modules",
        "cleanup:test": "pnpm dlx del-cli tests-results playwright-report",
        "cleanup:data": "pnpm dlx del-cli storage",
        "e2e-test:install": "playwright install --with-deps",
        "e2e-test:chrome": "playwright test --headed --project Chromium",
        "e2e-test:firefox": "playwright test --headed --project Firefox",
        "e2e-test:mobile": "playwright test --headed --project 'Mobile Chrome'",
        "e2e-test:safari": "playwright test --headed --project Safari",
        "e2e-test:report": "playwright show-report playwright-report",
        "e2e-test:ui": "playwright test --ui",
        "test:coverage": "vitest run --coverage",
        "test:report": "pnpm dlx http-server tests-results --port 0 -a localhost",
        "test:ui": "vitest --ui",
        "test": "vitest --run",
        "typecheck": "tsc -b --noEmit"
    },
    "dependencies": {
        "@radix-ui/react-slot": "^1.2.3",
        "@react-router/express": "^7.9.3",
        "@react-router/node": "^7.9.3",
        "clsx": "^2.1.1",
        "compression": "^1.8.1",
        "consola": "^3.4.2",
        "express-rate-limit": "^8.1.0",
        "express": "^5.1.0",
        "helmet": "^8.1.0",
        "isbot": "^5.1.31",
        "lucide-react": "^0.544.0",
        "morgan": "^1.10.1",
        "react-dom": "^19.2.0",
        "react-router": "^7.9.3",
        "react": "^19.2.0",
        "std-env": "^3.9.0",
        "tailwind-merge": "^3.3.1",
        "tailwind-variants": "^3.1.1"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.2.4",
        "@cloudflare/vite-plugin": "^1.13.8",
        "@playwright/test": "^1.55.1",
        "@react-router/dev": "^7.9.3",
        "@storybook/addon-a11y": "^9.1.10",
        "@storybook/addon-docs": "^9.1.10",
        "@storybook/addon-links": "^9.1.10",
        "@storybook/react-vite": "^9.1.10",
        "@tailwindcss/typography": "^0.5.19",
        "@tailwindcss/vite": "^4.1.14",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.0",
        "@types/compression": "^1.8.1",
        "@types/express-serve-static-core": "^5.0.7",
        "@types/express": "^5.0.3",
        "@types/morgan": "^1.9.10",
        "@types/node": "^24.6.2",
        "@types/react-dom": "^19.2.0",
        "@types/react": "^19.2.0",
        "@vitest/coverage-istanbul": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "dotenv": "^17.2.3",
        "git-cliff": "^2.10.1",
        "happy-dom": "^19.0.2",
        "lefthook": "^1.13.6",
        "npm-check-updates": "^19.0.0",
        "playwright": "^1.55.1",
        "rollup-plugin-visualizer": "^6.0.3",
        "storybook": "^9.1.10",
        "tailwindcss": "^4.1.14",
        "typescript": "^5.9.3",
        "vite-tsconfig-paths": "^5.1.4",
        "vite": "npm:rolldown-vite@7.1.14",
        "vitest": "^3.2.4"
    },
    "packageManager": "pnpm@10.17.1",
    "pnpm": {
        "overrides": {
            "vite": "npm:rolldown-vite@7.1.14"
        }
    }
}
